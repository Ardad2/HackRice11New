{
    "manifest_version": 2,
  
    "name": "Web Time Tracker",
    "description": "Your browser history time tracked!",
    "version": "1.0",
    "author": "Arjun Dadhwal",
  
    "permissions": [
      "tabs"
    ],
    "icons": {
    },
    "browser_action": {
      "default_icon": {
      }
    },
    "content_scripts": [{
      "css": ["styles.css"],
      "js": ["content.js"]
    }],
    "background": {
      "scripts": [
        "time.js",
        "background.js"
        ],
      "persistent": true
    },
    "options_page": "options.html"
  }

  